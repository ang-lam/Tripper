<h1>Sessions#new</h1>

<%= form_with url: login_path do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %> <br>
    <%= f.label :password %>
    <%= f.password_field :password %> <br>
    <%= f.submit 'Login' %>
<% end %>
<h3> Or Signup with Google:</h3>
<%= button_to "Google", '/auth/google_oauth2' %>
