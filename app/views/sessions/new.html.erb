<h1>Login</h1>

<br>

<%= form_with url: login_path do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %> <br><br>
    <%= f.label :password %>
    <%= f.password_field :password %> <br><br>
    <%= f.submit 'Login' %>
<% end %>

<br>

<h3> Or Login with Google:</h3>
<br>
<%= button_to "Google", '/auth/google_oauth2' %>
